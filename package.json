{
  "name": "abtt-wp",
  "version": "1.0.0",
  "description": "A WordPress theme template for developing custom ACF blocks.",
  "author": "Stephen Akinola <stephthedeveloper@gmail.com",
  "keywords":  [
    "wordpress",
    "theme",
    "acf",
    "blocks",
    "psr-4",
    "scss",
    "tailwindcss",
    "gulp"
  ],
  "license": "GPL-2.0-or-later",
  "rtlcssConfig": {
    "options": {
      "blacklist": {},
      "processUrls": false,
      "stringMap": []
    }
  },
  "scripts": {
