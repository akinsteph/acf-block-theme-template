    "lint:js": "wp-scripts lint-js 'js/*.js'"
  },
  "dependencies": {
    "gulp": "^4.0.2",
    "gulp-postcss": "^10.0.0",
    "gulp-sass": "^5.1.0",
    "sass": "^1.72.0"
  },
  "devDependencies": {
    "@wordpress/scripts": "^27.4.0",
    "autoprefixer": "^10.4.19",
    "gulp-clean-css": "^4.3.0",
    "tailwindcss": "^3.4.1"
  }
}
